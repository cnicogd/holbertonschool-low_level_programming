{
	va_list ap;
	t_fo ao[] = {
		{'s', _printstring},
		{'c', _printchar},

	};
	int i;
	va_star(ap, format);
	while ( *format)
	{
		if (*format != SPECIAL_CHAR)
		putchar(*format);
		format++;
		else
		{
			if(*format + 1 != NULL)
			{
				i = 0;
				while (ao[i].f)!= NULL)
				{
					if (*(format + 1) == ao[i].c)
					/* call the function*/
					ao[i].f();
					break
				}
		format += 2;
				}
		else/* this is null*/
		{
			format ++;
		}
		}
	}
	va_end(ap);
	return (0);
}